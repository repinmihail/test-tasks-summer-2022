over (partition by t.client order by t.client) as maximum\n","from (\n","  select\n","    t.*\n","    , coalesce(t.end_date, '2199-12-31') as end_date_upd\n","  from accounts as t) as t\n","where t.start_date <= '2018-03-01' and t.end_date_upd > '2018-03-01'\n","order by t.client),\n","\n","several_account_by_client as (\n","  select\n","    t.*\n","    , temp.account as max_acc\n","  from ( \n","    select\n","      t.client\n","      , t.account as acc1\n","      , x.account as acc2\n","      , t.cash\n","      , case when t.account != x.account then t.cash - x.cash else 0 end as diff\n","      , t.start_date\n","      , t.end_date_upd\n","    from update_tab as t\n","    join update_tab as x on t.client = x.client and t.account != x.account) as t\n","  left join (select client, account from update_tab where maximum=cash) as temp\n","      on t.client = temp.client), \n","\n","one_account_by_client as (\n","  select\n","    t.*\n","    , temp.account as max_acc\n","  from ( \n","    select\n","      t.client\n","      , t.account as acc1\n","      , x.account as acc2\n","      , t.cash\n","      , case when t.account != x.account then t.cash - x.cash else 0 end as diff\n","      , t.start_date\n","      , t.end_date_upd\n","    from update_tab as t\n","    join update_tab as x on t.client = x.client) as t\n","  left join (select client, account from update_tab where maximum=cash) as temp\n","      on t.client = temp.client\n","  where t.client in (select client from update_tab group by client having count(*)=1)\n",")\n","\n","select * from several_account_by_client\n","union\n","select * from one_account_by_client\n","order by client\n","\n","'''\n","# Примечание. Логика примера не до конца понятна: \n","# 1) 18 296 394 - 5000 = 18 921 394\n","# 2) при этом  5000 - 18 296 394 != -18 301 394 \n","# P.S.:При условии, что под разницей мы понимаем операцию вычитания одного из другого\n","# Кажется, что в обоих случаях разница между двумя числами должна быть одинаковой по модулю\n","# Если счетов больше, чем два, то тогда, если мы хотим видеть разницу по каждому \n","# счету по отношению к другому, то нужно использовать операцию cross join \n","# (хотя бы для тех клиентов, у которых счетов больше 2) - такая логика реализована в скрипте\n","pd.options.display.float_format = '{:,.0f}'.format\n","select(sql)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":582},"id":"mZARysJJTZJS","executionInfo":{"status":"ok","timestamp":1655043439446,"user_tz":-180,"elapsed":366,"user":{"displayName":"Михаил","userId":"15283324894937252744"}},"outputId":"ed3a9fbb-7662-440a-941e-3916f96b1bec"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["           client    acc1    acc2       cash        diff start_date  \\\n","0     Mr.Anderson 100,001 110,001 18,296,394  18,291,394 2016-01-01   \n","1     Mr.Anderson 110,001 100,001      5,000 -18,291,394 2009-01-01   \n","2       Mr.Corney 100,003 100,003  2,210,606           0 2016-01-01   \n","3       Mr.Darkon 100,004 100,004  1,755,177           0 2016-01-01   \n","4     Mr.Maddyson 100,011 100,011  1,699,680           0 2016-01-01   \n","5   Mr.Perestukin 100,015 100,015    800,990           0 2016-01-01   \n","6        Mr.Sammy 100,008 100,008  1,029,191           0 2016-01-01   \n","7        Mr.Smith 100,009 100,009    678,164           0 2016-01-01   \n","8    Mr.Zamkovsky 100,013 110,013    594,918     593,918 2016-01-01   \n","9    Mr.Zamkovsky 110,013 100,013      1,000    -593,918 2010-01-01   \n","10      Ms.Alovik 100,012 100,012    510,622           0 2016-01-01   \n","11      Ms.Duckky 100,018 100,018    719,514           0 2010-01-01   \n","12    Ms.Garryson 100,016 100,016  1,340,530           0 2016-01-01   \n","13        Ms.Gray 100,002 100,002  3,878,994           0 2016-01-01   \n","14       Ms.Kassy 100,007 100,007  2,192,545           0 2016-01-01   \n","15       Ms.Landa 100,005 100,005    625,308           0 2016-01-01   