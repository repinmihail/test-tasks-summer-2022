(\n","  select\n","    t.*\n","    , dense_rank() over (partition by t.client, t.account order by t.end_date) as rank\n","  from accounts as t\n","  where t.client in (select client from accounts where end_date is null)\n","  order by t.client) as t\n","where t.rank = 2),\n","\n","upd_table as (\n","  select\n","    t.account\n","    , t.client\n","    , f.cash\n","  from actual_field as t\n","  left join previous_field as f on t.client = f.client and t.account = f.account\n",")\n","\n","UPDATE accounts SET (cash) =\n","    (SELECT cash FROM upd_table\n","     WHERE upd_table.account = accounts.account)\n","'''\n","select(sql)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":520},"id":"QGdZvrV9H2m9","executionInfo":{"status":"ok","timestamp":1655048032525,"user_tz":-180,"elapsed":342,"user":{"displayName":"Михаил","userId":"15283324894937252744"}},"outputId":"30fd04ec-3264-49ba-c13b-c220c8498b6d"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["    account         client       cash\n","0   100,001    Mr.Anderson 12,197,596\n","1   100,003      Mr.Corney  1,473,737\n","2   100,004      Mr.Darkon  1,350,136\n","3   100,011    Mr.Maddyson  1,062,300\n","4   100,015  Mr.Perestukin    889,989\n","5   100,008       Mr.Sammy  1,143,546\n","6   100,009       Mr.Smith  1,130,273\n","7   100,013   Mr.Zamkovsky    991,530\n","8   110,013   Mr.Zamkovsky     91,530\n","9   100,012      Ms.Alovik  1,021,244\n","10  100,016    Ms.Garryson    837,831\n","11  100,002        Ms.Gray  4,848,742\n","12  100,007       Ms.Kassy  1,153,971\n","13  100,005       Ms.Landa  1,250,615\n","14  100,014      Ms.Yanina    973,891"],"text/html":["\n","  <div id=\"df-527c9c98-914a-44ee-9261-dc1c6293eed3\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: 